{
    "name": "gpt2",
    "ref_url": "https://openai.com/blog/better-language-models/",

    "image": {
        "maintainer": "jon@gauthiers.net",
        "version": "<image.sha1>",
        "checksum": "<image.files_sha1>",
        "datetime": "<image.datetime>",
        "supported_features": {
            "tokenize": true,
            "unkify": true,
            "get_surprisals": true,
            "get_predictions": true,
            "mount_checkpoint": false
        },
        "gpu": {
            "required": false,
            "supported": false
        }
    },

    "vocabulary": {
        "unk_types": [],
        "prefix_types": ["<|endoftext|>"],
        "suffix_types": ["<|endoftext|>"],
        "special_types": []
    },

    "tokenizer": {
        "type": "subword",
        "cased": true,
        "sentinel_position": "initial",
        "sentinel_pattern": "Ä "
    }
}
